{
  "name": "@commercelayer/cli-plugin-microstore",
  "description": "Commerce Layer CLI microstore plugin",
  "version": "3.0.0-rc.1",
  "author": "Pierluigi Viti <pierluigi@commercelayer.io>",
  "bugs": "https://github.com/commercelayer/commercelayer-cli-plugin-microstore/issues",
  "engines": {
    "node": ">=20"
  },
  "files": [
    "/bin/run.*",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/commercelayer/commercelayer-cli-plugin-microstore",
  "keywords": [
    "ecommerce",
    "cli",
    "commercelayer"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cl-microstore",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "plugins": [],
    "topics": {
      "microstore": {
        "description": "Generate microstore URLs",
        "hidden": false
      }
    },
    "repositoryPrefix": "<%- repo %>/blob/main/<%- commandPath %>",
    "additionalHelpFlags": [
      "-h"
    ],
    "topicSeparator": ":"
  },
  "repository": "commercelayer/commercelayer-cli-plugin-microstore",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "build": "rm -rf lib && tsc -b",
    "prepack": "pnpm build && oclif manifest && pnpm readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "readme": "cl-cli-dev readme --plugin --bin=commercelayer && git add README.md",
    "lint": "eslint src --ext .ts --config .eslintrc",
    "lint:fix": "eslint src --fix"
  },
  "types": "lib/index.d.ts",
  "devDependencies": {
    "@commercelayer/cli-dev": "^3.0.3",
    "@commercelayer/eslint-config-ts": "^1.4.5",
    "@oclif/plugin-help": "^6.0.21",
    "@oclif/test": "^3.2.11",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/chai": "^4.3.14",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.7",
    "chai": "^4.4.1",
    "eslint": "^8.57.0",
    "mocha": "^10.4.0",
    "nyc": "^15.1.0",
    "oclif": "^4.9.0",
    "semantic-release": "^23.0.8",
    "tsx": "^4.7.3",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@commercelayer/cli-core": "^5.0.0",
    "@commercelayer/sdk": "^6.0.0",
    "@oclif/core": "^3.26.4",
    "open": "^8.4.2",
    "tslib": "^2.6.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
